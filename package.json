{
  "name": "ScalaComputing",
  "version": "1.0.0",
  "description": "Job Interview Exercise",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/cypress open",
    "runRegressionLocal": "cypress run --spec \"cypress/integration/fsTable/*\" --reporter mochawesome",
    "runRegressionWeb": "cypress run --spec \"cypress/integration/fsTable/*\" --env configFile=awsinstance --reporter mochawesome",
    "run": "cypress run --reporter mochawesome",
    "cleanup": "rm -fr mochawesome-report",
    "merge_reports": "mochawesome-merge --reportDir mochawesome-report -o mochawesome-report/output.json",
    "generate_mochawesome_report": "marge --reportDir mochawesome-report mochawesome-report/output.json"
  },
  "author": "Yuliya Galperina",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^8.1.0",
    "electron": "^13.1.7"
  },
  "dependencies": {
    "json-query": "^2.2.2",
    "mocha": "^9.0.3",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "concurrently": "^6.2.0",
    "cypress-harvester": "^1.0.3"
  }
}
